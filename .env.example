# Strava API Configuration
STRAVA_CLIENT_ID=your_strava_client_id_here
STRAVA_CLIENT_SECRET=your_strava_client_secret_here
STRAVA_REDIRECT_URI=http://localhost:3000/callback

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/strava

# Frontend Environment Variables (comma-separated key=value pairs)
# These will be injected into the React build process
FRONTEND_ENV=REACT_APP_API_URL=http://localhost:8001,REACT_APP_STRAVA_CLIENT_ID=your_strava_client_id_here

# Legacy configuration (if needed)
SUPABASE_URL=
SUPABASE_KEY=
FRONTEND_URL=http://localhost:3000
BACKEND_DOCKER_URL=http://host.docker.internal:8001
MOCK_AUTH=true

# Production URLs (update these for your deployment)
# FRONTEND_ENV=REACT_APP_API_URL=https://your-app.herokuapp.com,REACT_APP_STRAVA_CLIENT_ID=your_strava_client_id_here
